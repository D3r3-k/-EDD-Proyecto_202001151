digraph G {
node [shape=box];
rankdir=TB;
label="Matriz de Relaciones de Usuarios" fontsize=20 fontname="Arial";
// Definir encabezados de filas
fila0 [label="Administrador", group=1];
fila1 [label="Usuario", group=1];
fila2 [label="Usuario2", group=1];
// Definir encabezados de columnas
col0 [label="Administrador", group=1];
col1 [label="Usuario", group=2];
col2 [label="Usuario2", group=3];
{ rank=same; col0; col1; col2; }
// Definir nodos en las coordenadas especificadas
nodo0_1 [label="", color=green, style=filled, group=2];
nodo1_0 [label="", color=green, style=filled, group=1];
nodo1_2 [label="", color=green, style=filled, group=3];
nodo2_1 [label="", color=green, style=filled, group=2];
// Conectar encabezados de filas con nodos y los nodos entre ellos horizontalmente
fila0 -> nodo0_1 [dir=both, minlen=2];
fila1 -> nodo1_0 [dir=both, minlen=2];
nodo1_0 -> nodo1_2 [dir=both, minlen=2];
fila2 -> nodo2_1 [dir=both, minlen=2];
// Conectar encabezados de columnas con nodos y los nodos entre ellos verticalmente
col0 -> nodo1_0 [dir=both, minlen=2];
col1 -> nodo0_1 [dir=both, minlen=2];
nodo0_1 -> nodo2_1 [dir=both, minlen=2];
col2 -> nodo1_2 [dir=both, minlen=2];
// Mantener los encabezados de filas alineados verticalmente
fila0 -> fila1 [style=invis];
fila1 -> fila2 [style=invis];
// Mantener los nodos de la misma fila en el mismo rango horizontal
{ rank=same; fila0; nodo0_1 }
{ rank=same; fila1; nodo1_0 }
{ rank=same; fila1; nodo1_2 }
{ rank=same; fila2; nodo2_1 }
// Conectar las columnas entre sÃ­ de forma invisible para mantener el orden
col0 -> col1 [style=invis];
col1 -> col2 [style=invis];
}
